#include<stdio.h>

int main()
{
    int year ;
    scanf("%d",&year);
    if (year%400==0)
    {printf("Einai  disekti imerominia !!");}
    else if (year%100==0)
    {printf("Einai disekti hmerominia !!");}
    else if (year%4==0)
    {printf("Einai disekti hmerominia !!");}
    else
    {printf("Den einai disekti !!!");}
    return 0;
}
